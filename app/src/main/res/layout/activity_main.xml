<?xml version="1.0" encoding="utf-8"?>
<androidx.wear.widget.BoxInsetLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#000000"
    android:padding="@dimen/box_inset_layout_padding"
    tools:context=".MainActivity"
    tools:deviceIds="wear">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TableLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:padding="30dp"
            android:stretchColumns="2">

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <TextView
                    android:id="@+id/textViewCel"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_marginStart="16dp"
                    android:drawableStart="@drawable/logo_cel"
                    android:text=" CEL"
                    android:textAlignment="textEnd"
                    android:textSize="14sp" />

                <TextView
                    android:id="@+id/textViewPrice_cel"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:drawableEnd="@drawable/increase"
                    android:paddingEnd="35dp"
                    android:text="-"
                    android:textAlignment="textEnd"
                    android:textColor="#FFC107"
                    android:textSize="14sp" />

            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <TextView
                    android:id="@+id/textViewHigh"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_marginStart="16dp"
                    android:text="High"
                    android:textAlignment="textEnd"
                    android:textSize="12sp" />

                <TextView
                    android:id="@+id/textViewHighPrice_cel"
                    android:layout_width="122dp"
                    android:layout_height="wrap_content"
                    android:paddingRight="55dp"
                    android:text="-"
                    android:textAlignment="textEnd"
                    android:textColor="#2BB201"
                    android:textSize="12sp" />

            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <TextView
                    android:id="@+id/textViewLow"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_marginStart="16dp"
                    android:text="Low"
                    android:textAlignment="textEnd"
                    android:textSize="12sp" />

                <TextView
                    android:id="@+id/textViewLowPrice_cel"
                    android:layout_width="122dp"
                    android:layout_height="wrap_content"
                    android:paddingRight="55dp"
                    android:text="-"
                    android:textAlignment="textEnd"
                    android:textColor="#F5123F"
                    android:textSize="12sp" />

            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <TextView
                    android:id="@+id/textViewBTC"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_marginStart="16dp"
                    android:drawableStart="@drawable/logo_btc"
                    android:text="BTC"
                    android:textAlignment="textEnd"
                    android:textSize="14sp" />

                <TextView
                    android:id="@+id/textViewPrice_btc"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:drawableEnd="@drawable/decrease"
                    android:paddingEnd="35dp"
                    android:text="-"
                    android:textAlignment="textEnd"
                    android:textColor="#FFC107"
                    android:textSize="14sp" />

            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="42dp">

                <TextView
                    android:id="@+id/textViewBTCHigh"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_marginStart="16dp"
                    android:text="High"
                    android:textAlignment="textEnd"
                    android:textSize="12sp" />

                <TextView
                    android:id="@+id/textViewHighPrice_btc"
                    android:layout_width="122dp"
                    android:layout_height="wrap_content"
                    android:paddingRight="55dp"
                    android:text="-"
                    android:textAlignment="textEnd"
                    android:textColor="#2BB201"
                    android:textSize="12sp" />

            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <TextView
                    android:id="@+id/textViewBTCLow"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_marginStart="16dp"
                    android:text="Low"
                    android:textAlignment="textEnd"
                    android:textSize="12sp" />

                <TextView
                    android:id="@+id/textViewLowPrice_btc"
                    android:layout_width="122dp"
                    android:layout_height="wrap_content"
                    android:paddingRight="55dp"
                    android:text="-"
                    android:textAlignment="textEnd"
                    android:textColor="#F5123F"
                    android:textSize="12sp" />

            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <TextView
                    android:id="@+id/textViewETH"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_marginStart="16dp"
                    android:drawableStart="@drawable/logo_eth"
                    android:text="ETH"
                    android:textAlignment="textEnd"
                    android:textSize="14sp" />

                <TextView
                    android:id="@+id/textViewPrice_eth"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:drawableEnd="@drawable/decrease"
                    android:paddingEnd="35dp"
                    android:text="-"
                    android:textAlignment="textEnd"
                    android:textColor="#FFC107"
                    android:textSize="14sp" />

            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <TextView
                    android:id="@+id/textViewETHHigh"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_marginStart="16dp"
                    android:text="High"
                    android:textAlignment="textEnd"
                    android:textSize="12sp" />

                <TextView
                    android:id="@+id/textViewHighPrice_eth"
                    android:layout_width="122dp"
                    android:layout_height="wrap_content"
                    android:paddingRight="55dp"
                    android:text="-"
                    android:textAlignment="textEnd"
                    android:textColor="#2BB201"
                    android:textSize="12sp" />

            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <TextView
                    android:id="@+id/textViewETHLow"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_marginStart="16dp"
                    android:text="Low"
                    android:textAlignment="textEnd"
                    android:textSize="12sp" />

                <TextView
                    android:id="@+id/textViewLowPrice_eth"
                    android:layout_width="122dp"
                    android:layout_height="wrap_content"
                    android:paddingRight="55dp"
                    android:text="-"
                    android:textAlignment="textEnd"
                    android:textColor="#F5123F"
                    android:textSize="12sp" />

            </TableRow>

        </TableLayout>
    </ScrollView>

</androidx.wear.widget.BoxInsetLayout>